import java.util.Scanner;
public class String2 {
    public static void main(String[] args){
        String str = "Namitha";
        char target = 'a';
        int count = 0;
        for(int i=0;i<str.length();i++){
            if(str.charAt(i) == target){
                count++;
            }
        }
        if(count>0){
            System.out.println(count + " times");
        }
    }
    
}
